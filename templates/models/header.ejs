<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no"/>
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">
<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/chart.js@3.8.0/dist/chart.min.js" integrity="sha256-cHVO4dqZfamRhWD7s4iXyaXWVK10odD+qp4xidFzqTI=" crossorigin="anonymous"></script>
<% if (production) { %>
    <script async defer data-website-id="1b82b7fe-3278-426e-818b-944684654c80" src="https://um.mooi.ng/um.js"></script>
<% } %>
<link rel="stylesheet" href="/dist/output.css" type="text/css">
<script src="/dist/tw/index.min.js"></script>
<script src="/static/js/fade.js"></script>
<script src="/static/js/authorization.js"></script>
<% if (image && embedSettings.enabled) { %>
    <meta name="twitter:card" content="summary_large_image">
    <meta property="og:image" content="<%= `/raw/${imageData.fileId}.${imageData.extension}` %>">
    <link type="application/json+oembed" href="/api/embed">
    <meta name="theme-color" content="<%= user.settings.color ? user.settings.color : '#000000' %>">
    <meta property="og:site_name" content="<%= embedSettings.siteName %>">
    <meta name="og:title" content="<%= embedSettings.title %>">
    <meta name="og:description" content="<%= embedSettings.description %>">
<% } else if (image) { %>
    <meta name="twitter:card" content="summary_large_image">
    <meta property="og:image" content="<%= `/raw/${imageData.fileId}.${imageData.extension}` %>">
    <meta name="twitter:image" content="<%= `/raw/${imageData.fileId}.${imageData.extension}` %>">
    <meta name="twitter:image:src" content="<%= `/raw/${imageData.fileId}.${imageData.extension}` %>">
<% } %>