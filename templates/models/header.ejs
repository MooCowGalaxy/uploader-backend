<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<link rel="icon" type="image/x-icon" href="/static/img/favicon.ico">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=5.0"/>
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
<% if (production) { %>
    <script async defer data-website-id="1b82b7fe-3278-426e-818b-944684654c80" src="https://um.mooi.ng/um.js"></script>
<% } %>
<link rel="stylesheet" href="/dist/output.css" type="text/css">
<script src="/static/js/fade.js"></script>
<script src="/static/js/authorization.js"></script>
<% if (image && embedSettings.enabled) { %>
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:image" content="<%= `https://cdn.uploader.tech/${imageData.ownerId}/${imageData.fileId}.${imageData.extension}` %>">
    <meta name="twitter:image:src" content="<%= `https://cdn.uploader.tech/${imageData.ownerId}/${imageData.fileId}.${imageData.extension}` %>">
    <meta property="og:image" content="<%= `https://cdn.uploader.tech/${imageData.ownerId}/${imageData.fileId}.${imageData.extension}` %>">
    <meta name="theme-color" content="<%= embedSettings.embedColor ? embedSettings.embedColor : '#000000' %>">
    <meta name="og:title" content="<%= embedSettings.embedSiteTitle %>">
    <meta name="twitter:title" content="<%= embedSettings.embedSiteTitle %>">
    <meta name="og:description" content="<%= embedSettings.embedSiteDescription %>">
    <meta name="twitter:description" content="<%= embedSettings.embedSiteDescription %>">
    <link type="application/json+oembed" href="https://uploader.tech/api/embed/<%= imageData.fileId %>">
<% } else if (image) { %>
    <meta name="twitter:card" content="summary_large_image">
    <meta property="og:image" content="<%= `https://cdn.uploader.tech/${imageData.ownerId}/${imageData.fileId}.${imageData.extension}` %>">
    <meta name="twitter:image" content="<%= `https://cdn.uploader.tech/${imageData.ownerId}/${imageData.fileId}.${imageData.extension}` %>">
    <meta name="twitter:image:src" content="<%= `https://cdn.uploader.tech/${imageData.ownerId}/${imageData.fileId}.${imageData.extension}` %>">
<% } else { %>
    <meta name="og:title" content="Uploader">
    <meta name="og:description" content="A free ShareX image host with multiple premium domains available for use with zero cost.">
    <meta name="description" content="A free ShareX image host with multiple premium domains available for use with zero cost.">
<% } %>