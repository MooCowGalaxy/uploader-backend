</head>
<body class="d-flex-col min-h-screen w-full">
<%- include('models/dashboardNavbar.ejs', {user}) %>
<div class="w-full text-black flex-1-1 d-flex-row relative">
    <%- include('models/sidebar.ejs', {}) %>
    <div id="container" class="p-5 bg-white">
        <div class="relative flex-initial">
            <div class="absolute rounded-md p-2 border border-slate-700 inline-block hover-cursor" id="toggle-sidebar-button">
