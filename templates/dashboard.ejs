                    <path stroke-linecap="round" stroke-linejoin="round" d="M4 6h16M4 12h16M4 18h16" />
                </svg>
            </div>
            <div class="flex justify-center">
                <h1 class="text-3xl font-semibold mb-5" id="page-title"></h1>
            </div>
        </div>
        <div id="content-container" class="flex-1 relative">
            <div id="page-home">

            </div>
            <div id="page-user"></div>
            <div id="page-embed"></div>
            <div id="page-gallery"></div>
            <div id="page-domains"></div>
            <div id="page-donate"></div>
        </div>
    </div>
</div>
<%- include('models/modal.ejs') %>
